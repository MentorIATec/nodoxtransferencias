{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/estudiante.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/estudiante",
      "dest": "/api/estudiante.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
